#include <stdio.h>

int main()
{
    int a, b, m, n, p, q;
    printf("Enter a:");
    scanf("%d", &a);
    printf("Enter b:");
    scanf("%d", &b);

    m = a + b;
    printf("m = %d\n", m);

    n = a * b;
    printf("n = %d\n", n);

    p = a - b;
    printf("p = %d\n", p);

    q = a / b;
    if(b != 0)
    {
        printf("q = %d\n", q);
    }
    else
        printf("error\n");
    
    return 0;
}
