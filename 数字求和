#include <stdio.h>

int main()
{
    int a[6];
    int n, sum;
    sum = 0;

    scanf("%d %d %d %d %d %d", &(a[0]), &(a[1]), &(a[2]), &(a[3]), &(a[4]), &(a[5]));

    for(n = 1; n <= 5; n++)
        {
            if((a[n])%(a[0]) == 0)
                sum = sum + (a[n]);
        }
    
    printf("%d", sum);

    return 0;
}
